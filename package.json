{
  "name": "nano-jsx",
  "version": "0.0.2",
  "description": "Lightweight 1KB JSX library",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run dev",
    "build": "npm run clean && tsc --project tsconfig.json",
    "bundle": "npm run build && webpack --config webpack/webpack.prod.js",
    "dev": "npm run build && npm-run-all --parallel dev:*",
    "dev:tsc": "tsc --project tsconfig.dev.json --watch",
    "dev:serve": "live-server --open=dev",
    "dev:bundle": " webpack --config webpack/webpack.dev.js --watch",
    "clean": "rimraf lib",
    "clean:test": "rimraf \"test/**/*.js\"",
    "prepublishOnly": "npm run build && npm test",
    "test": "npm run clean:test && tsc --project test/tsconfig.json && jest \"test/.+\\.test.js$\"  && npm run clean:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandeu/nano-jsx.git"
  },
  "keywords": [
    "mini",
    "micro",
    "nano",
    "tiny",
    "jsx",
    "react",
    "preact"
  ],
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yandeu/nano-jsx/issues"
  },
  "homepage": "https://github.com/yandeu/nano-jsx#readme",
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "funding": {
    "url": "https://github.com/sponsors/yandeu"
  }
}
