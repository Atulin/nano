{
  "name": "nano-jsx",
  "version": "0.0.7",
  "description": "Lightweight 1KB JSX library",
  "main": "lib/cjs/index.js",
  "scripts": {
    "start": "npm run dev",
    "ssr": "npm run dev & nodemon lib/cjs/devSSR.js",
    "tsc": "tsc --project tsconfig.json && tsc --project tsconfig.es2015.json",
    "build": "npm run tsc",
    "bundle": "npm-run-all bundle:*",
    "bundle:prod": "webpack --config webpack/webpack.bundle.prod.js",
    "bundle:core": "webpack --config webpack/webpack.bundle.core.js",
    "dev": "npm run build && npm-run-all --parallel dev:*",
    "dev:tsc": "tsc --project tsconfig.es2015.json --watch",
    "dev:serve": "live-server --open=dev",
    "dev:bundle": " webpack --config webpack/webpack.bundle.dev.js --watch",
    "dev:script": "webpack --config webpack/webpack.dev.js --watch",
    "clean": "rimraf lib",
    "clean:test": "rimraf \"test/**/*.js\"",
    "prepublishOnly": "npm run clean && npm run build && npm run bundle  && npm test",
    "test": "npm run clean:test && tsc --project test/tsconfig.json && jest \"test/.+\\.test.js$\"  && npm run clean:test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yandeu/nano-jsx.git"
  },
  "keywords": [
    "mini",
    "micro",
    "nano",
    "tiny",
    "jsx",
    "react",
    "preact"
  ],
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yandeu/nano-jsx/issues"
  },
  "homepage": "https://github.com/yandeu/nano-jsx#readme",
  "dependencies": {
    "htm": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "jsdom": "^16.2.2",
    "live-server": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^5.0.0-beta.18",
    "webpack-cli": "^4.0.0-beta.8"
  },
  "engines": {
    "node": ">=12"
  },
  "engineStrict": true,
  "funding": {
    "url": "https://github.com/sponsors/yandeu"
  }
}
